{
  "name": "@dragoscirjan/configs",
  "version": "1.0.0",
  "description": "## Mac OS",
  "main": "dist/node-cjs/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test  NO_API_DOC=1 jest --coverage --runInBand --verbose",
    "clean": "rimraf ./dist",
    "prepare": "husky",
    "release": "run-s release:*",
    "release:release-it": "release-it --ci --no-npm.publish",
    "release:release-please": "release-please",
    "test:watch": "npm run test -- --watch",
    "build": "run-s clean build:*",
    "build:browser": "tsc -p ./tsconfig.browser.json",
    "build:browser-bundle": "esbuild --bundle dist/browser/index.js --format=esm --target=es2020 --outfile=dist/browser/index.bundle.js",
    "build:browser-bundle-min": "esbuild --minify --bundle dist/browser/index.js --format=esm --target=es2020 --outfile=dist/browser/index.bundle.min.js",
    "build:browser-umd": "rollup dist/browser/index.bundle.js --format umd --name \"@templ-project/node-typescript\" -o dist/browser/index.umd.js",
    "build:browser-umd-min": "rollup dist/browser/index.bundle.min.js --compact --format umd --name \"@templ-project/node-typescript\" -o dist/browser/index.umd.min.js",
    "build:node-cjs": "tsc -p ./tsconfig.cjs.json",
    "build:node-esm": "tsc -p ./tsconfig.esm.json && node .scripts/esm-module.js",
    "build:types": "tsc -p ./tsconfig.types.json",
    "ca": "run-s ca:lint ca:quality ca:security",
    "ca:lint": "run-s lint prettier",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint ./{src,test}/**/*.ts --fix",
    "lint:oxlint": "oxlint --jest-plugin --deny-warnings ./{src,test}/**/*.ts",
    "prettier": "prettier ./{src,test*}/**/*.ts --write",
    "ca:quality": "run-s jscpd depcruise",
    "jscpd:html": "npm run jscpd -- --reporters html",
    "jscpd": "jscpd ./src --blame",
    "depcruise": "depcruise --config .dependency-cruiser.js src",
    "ca:security": "run-s license-check audit-modules",
    "license-check": "npx license-checker --production --json --failOn=\"AGPL AGPL 1.0; AGPL 3.0; CDDL or GPLv2 with exceptions; CNRI Python GPL Compatible; Eclipse 1.0; Eclipse 2.0; GPL; GPL 1.0; GPL 2.0; GPL 2.0 Autoconf; GPL 2.0 Bison; GPL 2.0 Classpath; GPL 2.0 Font; GPL 2.0 GCC; GPL 3.0; GPL 3.0 Autoconf; GPL 3.0 GCC; GPLv2 with XebiaLabs FLOSS License Exception; LGPL; LGPL 2.0; LGPL 2.1; LGPL 3.0; Suspected Eclipse 1.0; Suspected Eclipse 2.0\"",
    "audit-modules": "yarn audit --groups=dependencies",
    "nvim": "cross-env NEOVIM_LANG=typescript nvim ."
  },
  "author": "Dragos Cirjan <dragos.cirjan@cellebrite.com>",
  "license": "ISC",
  "repository": "git@github.com:dragoscirjan/configs.git",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jscpd/badge-reporter": "^3.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "core-js": "^3.36.1",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^16.3.1",
    "esbuild": "^0.20.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonar": "^0.13.2",
    "eslint-plugin-sonarjs": "^0.25.1",
    "globby": "^14.0.1",
    "husky": "^9.0.11",
    "import-sort-style-module": "^6.0.0",
    "jest": "^29.7.0",
    "jscpd": "^3.5.10",
    "license-checker": "^25.0.1",
    "nodemon": "^3.1.0",
    "npm-run-all2": "^6.1.2",
    "nyc": "^15.1.0",
    "oxlint": "^0.2.17",
    "prettier": "^3.2.5",
    "prettier-plugin-import-sort": "^0.0.7",
    "release-it": "^17.2.0",
    "release-please": "^16.10.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "browser": "./dist/browser/index.js",
      "worker": "./dist/browser/index.js",
      "bun": "./dist/browser/index.js",
      "deno": "./dist/worker/index.js",
      "require": "./dist/node/node-cjs/index.js",
      "import": "./dist/node/node-esm/index.js"
    }
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "dependencies": {
    "axios": "^1.6.8",
    "winston": "^3.13.0"
  }
}
